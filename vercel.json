{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/main/.*",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/api/login/.*",
            "dest": "/routes/loginRouter.js"
        },
        {
            "src": "/api/registration/.*",
            "dest": "/routes/registrationRouter.js"
        },
        {
            "src": "/api/dashboard/.*",
            "dest": "/routes/dashboardRouter.js"
        },
        {
            "src": "/",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/about",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/services",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/testimonials",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/contact",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/blog",
            "dest": "/routes/mainRouter.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        }
    ]
}